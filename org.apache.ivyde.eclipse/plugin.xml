      </command>
      <command
            categoryId="org.apache.ivyde.commands"
            id="org.apache.ivyde.commands.reloadsettings"
            name="Reload settings">
      </command>
      <command
            categoryId="org.apache.ivyde.commands"
            id="org.apache.ivyde.commands.resolveall"
